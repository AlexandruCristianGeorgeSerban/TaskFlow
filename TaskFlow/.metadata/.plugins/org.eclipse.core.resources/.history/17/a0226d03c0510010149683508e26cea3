public class RootGroup {
    private String name;
    private String colorHex;
    private List<Category> categories = new ArrayList<>();

    public RootGroup(String name, String colorHex) {
        this.name = name;
        this.colorHex = colorHex;
    }
    // getters/setters
    @Override
    public String toString() {
        return name;
    }
}
